{% extends 'AdminBundle:Default:index.html.twig' %}

{% block body %}
    <h1>Setting</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ setting.id }}</td>
            </tr>
            <tr>
                <th>Yourteam</th>
                <td>{{ setting.yourTeam }}</td>
            </tr>
            <tr>
                <th>Roundnumber</th>
                <td>{{ setting.roundNumber }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('setting_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('setting_edit', { 'id': setting.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
