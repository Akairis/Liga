                                                        $('.match').append("<tr><td><b>9 minuta</b></td><td><img style='width: 20px;' src='http://img03.deviantart.net/391b/i/2012/147/a/f/soccer_ball_animated_by_indramjha-d518u1w.gif'/></td><td>" + desc + " </td></tr>").fadeIn('slow');
                                                    }

                                    //                if(data.length > 5){
                                    //                    // jeżeli jest 6 wpisów to usuwamy najstarszy
                                    //                    $('.desc').last().fadeOut(100);
                                    //                }
                                                });
                                            }

                                            $(function(){
                                                refreshData();

                                                setInterval(function(){
                                                    refreshData();
                                                }, 4000);
                                            });

